{
  "segmentation": {
    "architecture": {
      "head_A_class_count": 15,
      "head_B_class_count": 3,
      "subhead_count": 1,
      "trunk": {
        "convolution_size": 3,
        "padding": 1,
        "stride": 1,
        "batch_norm": true,
        "batch_norm_tracking": true
      }
    },
    "preprocessor": {
      "prescale_all": false,
      "prescale_factor": 0.33,
      "sobelize": false,
      "jitter_brightness": 0.4,
      "jitter_contrast": 0.4,
      "jitter_saturation": 0.4,
      "jitter_hue": 0.125
    },
    "transformations": {
      "flip_horizontal_probability": 0.5,
      "use_random_affine": true,
      "rotation_range": [-30.0, 30.0],
      "shear_range": [-10.0, 10.0],
      "scale_range": [0.8, 1.2]
    },
    "training": {
      "batch_size": 120,
      "shuffle": true,
      "num_dataloaders": 1,
      "num_epochs": 5,
      "validation_mode": "IID",
      "head_order": ["A", "B"],
      "loss": {
        "lambs": { "A": 1.0, "B": 1.5 },
        "use_uncollapsed": true,
        "half_T_side_dense": 10,
        "half_T_side_sparse_min": 0,
        "half_T_side_sparse_max": 0
      }
    },
    "dataset": {
      "name": "OTHER",
      "id": 777,
      "name_bak": "Coco164kCuratedFew",
      "root": "./datasets/test",
      "partitions": {
        "train": { "train": "train2017", "test": "val2017" },
        "map_assign": { "train": "train2017", "test": "val2017" },
        "map_test": { "train": "train2017", "test": "val2017" }
      },
      "parameters": {
        "coco_164k_curated_version": 6,
        "fine_to_coarse_dict": "./datasets/cocostuff/fine_to_coarse_dict",
        "use_coarse_labels": true,
        "include_things_labels": false,
        "include_animal_things": false,
        "include_rgb": false,
        "using_IR": false,
        "mask_input": false,
        "input_size": 128
      }
    },
    "optimizer": {
      "name": "Adam",
      "learning_rate": 0.0001,
      "schedule": [],
      "multiplier": 0.1
    },
    "output": {
      "batch_print_freq": 5,
      "save_freq": 1,
      "rendering": {
        "output_subfolder": "renders",
        "enabled": true
      },
      "root": "./out",
      "plot_name": "plots.png"
    },
    "debug": {
      "enabled": false
    }
  }
}
